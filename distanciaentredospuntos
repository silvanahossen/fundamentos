#include <iostream>
#include <math.h>

using namespace std;

int main()
{
   float x1, x2,y1,y2;
   double dx, dy, distancia;
   cout<<"ingresar el primer par ordenado : ";cin>>x1;cin>>y1;
   cout<<"ingresar el segundo par ordenado : ";cin>>x2;cin>>y2;
   dx=x2-x1;
   dy=y2-y1;
   distancia=sqrt(pow((dx),2)+pow((dy),2));
   cout<<"la distancia entre las dos rectas es : "<<distancia;
    return 0;
}
